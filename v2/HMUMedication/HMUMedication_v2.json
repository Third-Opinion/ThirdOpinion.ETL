{
  "Name": "HMUMedication_v2",
  "JobMode": "SCRIPT",
  "Description": "HMUMedication ETL v2 with medication code enrichment",
  "Role": "arn:aws:iam::442042533707:role/service-role/AWSGlueServiceRole",
  "ExecutionProperty": {
    "MaxConcurrentRuns": 3
  },
  "Command": {
    "Name": "glueetl",
    "ScriptLocation": "s3://aws-glue-assets-442042533707-us-east-2/scripts/HMUMedication_v2.py",
    "PythonVersion": "3"
  },
  "DefaultArguments": {
    "--enable-metrics": "true",
    "--enable-spark-ui": "false",
    "--enable-job-insights": "true",
    "--enable-debug-logging": "false",
    "--enable-observability-metrics": "true",
    "--enable-glue-datacatalog": "true",
    "--enable-continuous-cloudwatch-log": "true",
    "--job-bookmark-option": "job-bookmark-disable",
    "--datalake-formats": "iceberg",
    "--job-language": "python",
    "--TempDir": "s3://aws-glue-assets-442042533707-us-east-2/temporary/",
    "--extra-py-files": "s3://aws-glue-assets-442042533707-us-east-2/python-libs/HMUMedication_v2.zip",
    "--TABLE_NAME_POSTFIX": "_v2",
    "--conf": "spark.sql.adaptive.enabled=true",
    "--conf": "spark.sql.adaptive.coalescePartitions.enabled=true",
    "--conf": "spark.sql.shuffle.partitions=400",
    "--conf": "spark.sql.adaptive.skewJoin.enabled=true",
    "--conf": "spark.shuffle.compress=true",
    "--conf": "spark.serializer=org.apache.spark.serializer.KryoSerializer",
    "--conf": "spark.sql.adaptive.advisoryPartitionSizeInBytes=128MB",
    "--conf": "spark.dynamicAllocation.enabled=true",
    "--conf": "spark.dynamicAllocation.minExecutors=5",
    "--conf": "spark.dynamicAllocation.maxExecutors=30"
  },
  "Connections": {
    "Connections": [
      "Network connection",
      "Redshift connection"
    ]
  },
  "MaxRetries": 0,
  "Timeout": 720,
  "GlueVersion": "4.0",
  "NumberOfWorkers": 6,
  "WorkerType": "G.2X",
  "ExecutionClass": "FLEX"
}

